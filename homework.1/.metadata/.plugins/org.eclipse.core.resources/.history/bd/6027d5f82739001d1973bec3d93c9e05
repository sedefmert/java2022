
public class Main {

	public static void main(String[] args) {
		
		// for loop
		
		
		for(int i=1;i<=10;i++) {
			System.out.println(i);
		}
		System.out.println("for döngüsü bitti.");
		
		//çift sayılar 1-10 arasında
		
		for(int i=0;i<=10;i+=2) {
			System.out.println(i);
		}
		System.out.println("for döngüsü bitti.");
		
		
		//  while loop
		
		
		
		
		
	}

}
